.Palace{
    display: flex;
}
.box{
    width: 354rpx;
    height: 344rpx;
    margin: 10rpx 20rpx;
    background-color: #fff;
    border-radius: 8rpx;
    box-shadow: darkgrey 0px 0px 15px 5px;
    display: flex;
    justify-content: center;
    flex-direction: column;
    font-size: 40rpx;
}
.box>image{
    width: 140rpx;
    height: 140rpx;
    margin-left:110rpx;
    margin-top:46rpx;
}
.box>view{
    display: flex;
    justify-content: center;
}